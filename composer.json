 {
    "name": "crevillo/ezplatform-captcha",
    "description": "Repository dedicated add captcha to some parts of eZ Platform",
    "type": "bundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Carlos Revillo <crevillo@gmail.com>",
            "homepage": "https://github.com/crevillo/ezplatform-captcha/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Crevillo\\EzCaptchaBundle\\": "src/bundle/"
        }
    },
    "require": {
        "php": "^7.1",
        "gregwar/captcha-bundle": "2.*",
        "ezsystems/ezpublish-kernel": "~7.5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.7.1",
        "phpunit/phpunit": "^6.4"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
